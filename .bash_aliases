alias l.='ls -d .* --color=auto'
alias ll='ls -l --color=auto'

#c++ aliases
alias clang++='clang++-3.8'

#git aliases
alias com='git commit -a'
alias push='git push'
alias pull='git pull'

#qemu aliases
alias qem='make clean && make qemu'

#c alias for make clean and make
alias mk='make clean && make'

#pycharm alias
alias pycom='py3clean . && com'
alias pyrun='python3 manage.py runserver'
alias pymig='python3 manage.py migrate'
alias pyload='python3 manage.py loaddata'
alias pymakem='python3 manage.py makemigrations'


